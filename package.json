  "private": false,
  "scripts": {
    "webpack:build": "webpack --mode=production --progress",
    "build": "ENV=production gulp build && yarn run dist:widget",
    "build:snippet": "WIDGETS_URL=../scripts/widgets envify app/scripts/embeddable/snippet.js | uglifyjs -m -c unused,booleans,conditionals,dead_code,if_return,join_vars > build/scripts/snippet.js",
    "build:widget": "webpack --mode development --watch",
    "dist": "yarn run dist:static && yarn run dist:widget",
    "dist:static": "mkdir -p ./dist && ENV=production gulp dist",
    "dist:snippet": "WIDGETS_URL=http://assets.kiiiosk.ru/widgets envify app/scripts/embeddable/snippet.js | uglifyjs -m -c unused,booleans,conditionals,dead_code,if_return,join_vars > dist/scripts/snippet.js",
    "dist:test": "yarn run dist && yarn run test",
    "dist:widget": "webpack --mode production",
    "test": "gulp test && node ./dist_tests/prerender",
    "deploy": "gulp deploy",
    "start": "yarn run build:widget & yarn run server",
    "server": "gulp server",
    "start:dev": "webpack-dev-server --mode development",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'app/bower_components', 'junction') } catch (e) { }\""
  },
