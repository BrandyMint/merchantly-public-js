{
  "presets": [
    "react",
    ["env",
      {
       "targets" : {
          "browsers": ["last 3 versions", "ie >= 8"],
          "node": "current"
        },
        "debug": false
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime",
    "add-module-exports",
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "scripts": "./app/scripts",
        "r": "./app/scripts/react",
        "rc": "./app/scripts/react/components",
        "test": "./test",
        "styles": "./app/stylesheets"
      }
    }]
  ]
}
