{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", { "debug": false, "useBuiltIns": "entry" } ]
  ],
  "plugins": [
    "@babel/transform-runtime",
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    "@babel/plugin-proposal-class-properties",
    "add-module-exports",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "scripts": "./app/scripts",
        "r": "./app/scripts/react",
        "rc": "./app/scripts/react/components",
        "test": "./test",
        "styles": "./app/stylesheets"
      }
    }]
  ]
}
